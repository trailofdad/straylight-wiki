    "babel-preset-latest": "^6.24.1",
    "gatsby": "^2.13.25",
    "gatsby-link": "^2.2.2",
    "gatsby-plugin-catch-links": "^2.1.2",
    "gatsby-plugin-feed": "^2.3.4",
